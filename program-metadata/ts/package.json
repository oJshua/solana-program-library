    "prettier": "^2.2.1",
    "save-dev": "0.0.1-security",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.35",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@solana/web3.js": "^1.2.6",
    "@types/bs58": "^4.0.1",
